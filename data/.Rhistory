calc_binomzn <- function(p, n) {
phat <- vector(0:n)/n
zn <- (phat - p) / (sqrt((p(1-p))/n))
Fbin <- pbinom(n*p + sqrt(n*p*(1-p))*zn, n, p)
return(list(zn = zn, Fbin = Fbin))
}
